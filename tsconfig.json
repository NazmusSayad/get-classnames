{
	"compilerOptions": {
		"target": "es6",
		"skipLibCheck": true,
		"declaration": true,
		"inlineSourceMap": false,
		"strict": true,
		"pretty": true,
		"removeComments": true,
		"moduleResolution": "node"
	},
	"include": [
		"src/**/*.js",
		"src/**/*.jsx",
		"src/**/*.ts",
		"src/**/*.tsx"
	]
}